{
    "after-scripts": [
        "brew cask cleanup",
        "brew cask update",
        "brew cleanup",
        "brew update",
        "brew bundle --global --verbose"
    ],
    "symlinks": {
        ".Brewfile": "~",
        ".snippets": "~",
        "anyenv": "~/.anyenv",
        "git/.*": "~",
        "mackup/.*": "~",
        "tmux/.*": "~",
        "tmux/tpm": "~/.tmux/plugins/tpm",
        "vim/.*": "~",
        "vim/*.toml": "~/.vim/",
        "vim/load": "~/.vim/load",
        "vimperator/.vimperatorrc.local": "~",
        "vimperator/": "~/.vimperator/colors",
        "zsh/.*": "~"
    },
    "taps": [
        "Homebrew/bundle"
    ]
}
